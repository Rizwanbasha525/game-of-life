stage('git') {
   git 'https://github.com/Rizwanbasha525/game-of-life.git'
 }
stage('packege') {
   sh'mvn package'
}
stage('docker build') {
  sh  'docker build -t shaikbash/game:0.1'
}
stage('docker push image'){
withCredentials([string(credentialsId: '', variable: 'dockerpwd')]) {
   sh "docker login -u shaikbash -p ${dockerpwd}"
}
   sh 'docker push shaikbash/game:o.1'
}
